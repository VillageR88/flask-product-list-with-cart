{
    "version": 2,
    "builds": [
        {
            "src": "app/freeze.py",
            "use": "@vercel/python"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/chat/",
            "dest": "/app/freeze.py"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    }
}